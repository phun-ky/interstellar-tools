  "scripts": {
    "docs:partials:table:asteroid-belts": "node scripts/partials/dataset/asteroid-belts.mjs",
    "docs:partials:table:asteroids": "node scripts/partials/dataset/asteroids.mjs",
    "docs:partials:table:comets": "node scripts/partials/dataset/comets.mjs",
    "docs:partials:table:galaxies": "node scripts/partials/dataset/galaxies.mjs",
    "docs:partials:table:moons": "node scripts/partials/dataset/moons.mjs",
    "docs:partials:table:planets": "node scripts/partials/dataset/planets.mjs",
    "docs:partials:table:stars": "node scripts/partials/dataset/stars.mjs",
    "docs:partials:table:systems": "node scripts/partials/dataset/systems.mjs",
    "predocs:partials:gen": "npm run build",
    "docs:partials:gen": "npm run docs:partials:table:asteroid-belts && npm run docs:partials:table:asteroids && npm run docs:partials:table:comets && npm run docs:partials:table:galaxies && npm run docs:partials:table:moons && npm run docs:partials:table:planets && npm run docs:partials:table:stars && npm run docs:partials:table:systems",
    "prebuild": "npm run clean",
    "build": "tsc -b",
    "clean": "npm run clean --workspaces --if-present",
    "commit": "npx git-cz",
    "predocs:gen": "npm run docs:partials:gen",
    "docs:gen": "node ./node_modules/.bin/typedoc",
    "predocs:dev": "npm run docs:gen",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prerelease": "npm run build && npm run docs:gen && git add .",
    "release": "npm run release --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "test:ci": "npm run test:ci --workspaces --if-present"
  },
  "devDependencies": {
    "@release-it/bumper": "^7.0.5",
